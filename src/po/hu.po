#: ../src/gtk.c:2826
#, c-format
msgid "Reset to _Default (%d)"
msgstr "Visszaállítás az ala_pértelmezettre (%d)"

#: ../src/mp.c:1469
msgid "Negative X and non-integer Y not supported"
msgstr "Negatív X és nem-egész Y nem támogatott"

