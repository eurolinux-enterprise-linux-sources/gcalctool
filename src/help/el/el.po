msgstr "2"

#: C/gcalctool.xml:234(para)
msgid "Octal"
msgstr "Οκταδικό"

#: C/gcalctool.xml:235(para)
msgid "8"
msgstr "8"

#: C/gcalctool.xml:238(para)
msgid "Decimal"
msgstr "Δεκαδικό"

#: C/gcalctool.xml:239(para)
msgid "10"
msgstr "10"

#: C/gcalctool.xml:242(para)
